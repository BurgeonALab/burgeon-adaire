{
  "name": "burgeon-adaire",
  "version": "1.9.0",
  "description": "PT. Burgeon Adaire International thrives in a dynamic landscape, offering insights and opportunities while building strong partnerships and consistently delivering exceptional value.",
  "main": "./src/index.js",
  "scripts": {
    "build": "del-cli --force ./compressed && webpack --config webpack.config.js --env NODE_ENV=production",
    "s3-script": "concurrently \"aws s3 rm s3://burgeonadaire-storage/compiled --recursive\" \"aws s3 rm s3://burgeonadaire-storage/compressed --recursive\" \"aws s3 cp ./dist/compiled s3://burgeonadaire-storage/compiled --recursive\" \"aws s3 cp ./compressed s3://burgeonadaire-storage/compressed --recursive\" \"aws s3 cp ./dist/configurations s3://burgeonadaire-storage --recursive\"",
    "s3-images": "aws s3 cp ./dist/assets/images s3://burgeonadaire-storage --recursive",
    "s3-videos": "aws s3 cp ./dist/assets/videos s3://burgeonadaire-storage --recursive",
    "s3-media": "concurrently \"npm run s3-images\" \"npm run s3-videos\"",
    "s3-new-images": "aws s3 cp ./dist/assets/images/new_bai_img s3://burgeonadaire-storage/new_bai_img --recursive",
    "s3-new-videos": "aws s3 cp ./dist/assets/videos/new_bai_vid s3://burgeonadaire-storage/new_bai_vid --recursive",
    "s3-media-new": "concurrently \"npm run s3-new-images\" \"npm run s3-new-videos\"",
    "start-live": "npx webpack serve --config webpack.config.js --env NODE_ENV=development",
    "firebase-emulator": "firebase emulators:start",
    "deploy": "firebase deploy",
    "build:server:dev": "webpack --config ssr-webpack/ssr.server.js --mode development --env=development",
    "build:server:prod": "webpack --config ssr-webpack/ssr.server.js --mode production --env=production",
    "build:client:dev": "webpack --config ssr-webpack/ssr.client.js --mode development --env=development",
    "build:client:prod": "webpack --config ssr-webpack/ssr.client.js --mode production --env=production",
    "build:ssr:dev": "npm run build:client:dev && npm run build:server:dev",
    "build:ssr:prod": "npm run build:client:prod && npm run build:server:prod",
    "start:ssr:dev": "concurrently \"npm run build:server:dev && nodemon --exec babel-node src/server/index.js\"  \"npm run build:client:dev -- --watch\"",
    "start:ssr:prod": "concurrently \"npm run build:server:prod && nodemon --exec babel-node src/server/index.js\"  \"npm run build:client:prod -- --watch\""
  },
  "keywords": [],
  "author": "PT. Burgeon Adaire International",
  "license": "MIT and GPL-3.0",
  "devDependencies": {
    "@babel/node": "^7.28.0",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-brands-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.13.5",
    "compression-webpack-plugin": "^11.1.0",
    "concurrently": "^9.2.1",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "dedent": "^1.7.0",
    "del-cli": "^6.0.0",
    "html-webpack-plugin": "^5.6.4",
    "mini-css-extract-plugin": "^2.9.4",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "react-refresh": "^0.17.0",
    "style-loader": "^4.0.0",
    "svg-url-loader": "^8.0.0",
    "swc-loader": "^0.2.6",
    "terser-webpack-plugin": "^5.3.14",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "bootstrap": "^5.3.8",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "firebase": "^12.2.1",
    "gsap": "^3.13.0",
    "jquery": "^3.7.1",
    "moment": "^2.30.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-fast-marquee": "^1.6.5",
    "react-helmet-async": "^2.0.5",
    "react-in-viewport": "^1.0.0-beta.8",
    "react-router-dom": "^7.8.2",
    "react-slick": "^0.31.0",
    "react-svg": "^16.3.0",
    "react-ts-tradingview-widgets": "^1.2.8",
    "vanilla-cookieconsent": "^3.1.0"
  },
  "overrides": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  }
}
